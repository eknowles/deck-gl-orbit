{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "PORT=3001 bun --hot src/index.tsx",
    "build": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/index.tsx"
  },
  "dependencies": {
    "@deck.gl/core": "^9.1.14",
    "@deck.gl/layers": "^9.1.14",
    "@deck.gl/react": "^9.1.14",
    "debug": "^4.4.1",
    "deck.gl": "^9.1.14",
    "geolib": "^3.3.4",
    "mapbox-gl": "^3.14.0",
    "maplibre-gl": "^5.7.0",
    "react": "^19",
    "react-dom": "^19",
    "react-map-gl": "^8.0.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/debug": "^4.1.12",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
